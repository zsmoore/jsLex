{
  "name": "lexr",
  "version": "0.6.3",
  "description": "Lexical analyzer built in Javascript",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "test": "nyc --reporter=html --reporter=text mocha --require babel-register --require babel-polyfill",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint -c .eslintrc.json --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zsmoore/lexr.git"
  },
  "keywords": [
    "lex",
    "lexer",
    "lexical",
    "analysis",
    "scan",
    "scanner",
    "scanning",
    "token",
    "tokenize",
    "tokenizer",
    "flex",
    "language support",
    "built in",
    "javascript",
    "lexical"
  ],
  "author": "Zachary Moore",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zsmoore/lexr/issues"
  },
  "homepage": "https://github.com/zsmoore/lexr#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1"
  },
  "dependencies": {}
}
